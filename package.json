{
  "name": "webassembly-proposals",
  "description": "WebAssembly proposals tracking repository",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebAssembly/proposals.git"
  },
  "keywords": [
    "WebAssembly",
    "proposals"
  ],
  "author": "Sergey Rubanov",
  "bugs": {
    "url": "https://github.com/WebAssembly/proposals/issues"
  },
  "homepage": "https://github.com/WebAssembly/proposals#readme",
  "devDendencies": {
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  }
}
